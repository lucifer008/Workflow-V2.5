/*- Menu Tabs B--------------------------- */

.tabsB {
      float:left;
      width:100%;
      background:#F4F4F4;
      font-size:93%;
      line-height:normal;
      }
.tabsB ul {
	margin:0;
	padding:10px 10px 0 50px;
	list-style:none;
      }
.tabsB li {
      display:inline;
      margin:0;
      padding:0;
      }
.tabsB a {
      float:left;
      background:url(images/tableftB.gif) no-repeat left top;
      margin:0;
      padding:0 0 0 4px;
      text-decoration:none;
      }
.tabsB a span {
      float:left;
      display:block;
      background:url(images/tabrightB.gif) no-repeat right top;
      padding:5px 15px 4px 6px;
      color:#666;
      }
    /* Commented Backslash Hack hides rule from IE5-Mac \*/
.tabsB a span {float:none;}
    /* End IE5-Mac hack */
.tabsB a:hover span {
      color:#000;
      }
.tabsB a:hover {
      background-position:0% -42px;
      }
.tabsB a:hover span {
      background-position:100% -42px;
      }