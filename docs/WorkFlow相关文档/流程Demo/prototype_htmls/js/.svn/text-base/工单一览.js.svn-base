//新增工单
function newOrderForm()
{
	window.open('NewOrder.html');
}
//设置默认条件
function setDefaultCondition()
{
	window.showModalDialog('SetDailyOrderDefaultCondition.html',null,"dialogHeight:250px");
}
//结算
function closeOrderForm()
{
	window.open('../finance/UpdateOrder.html');
}
