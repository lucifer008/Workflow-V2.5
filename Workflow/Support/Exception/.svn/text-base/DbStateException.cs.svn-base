using System;
using System.Collections.Generic;
using System.Text;

namespace Workflow.Support.Exception
{
    public class DbStateException : WorkflowException
    {
        public DbStateException() : base()
        {}

        public DbStateException(string message)
            : base(message)
        { }

        public DbStateException(string message, System.Exception exception)
            : base(message, exception)
        { }
    }
}
