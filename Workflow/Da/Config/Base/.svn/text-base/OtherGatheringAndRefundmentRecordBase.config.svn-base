				INSERT_USER,
				UPDATE_DATE_TIME,
				UPDATE_USER,
				DELETED,
				VERSION,
				BRANCH_SHOP_ID,
				COMPANY_ID
			) VALUES
			( 
				#Id#,
				#OrdersId#,
				#CustomerId#,
				#MemberCardId#,
				#Amount#,
				#TicketAmountSum#,
				#PayKind#,
				#Memo#,
				#InsertDateTime#,
				#InsertUser#,
				#UpdateDateTime#,
				#UpdateUser#,
				#Deleted#,
				#Version#,
				#BranchShopId#,
				#CompanyId#
			)
