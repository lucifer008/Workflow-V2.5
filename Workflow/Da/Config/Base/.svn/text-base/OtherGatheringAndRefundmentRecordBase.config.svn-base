				SET	VERSION = VERSION + 1,DELETED = '1'	WHERE ID = #Id#
			</update>

			<!--物理删除表OTHER_GATHERING_AND_REFUNDMENT_RECORD [其它收退款记录]的基本操作-->
			<delete id="Delete" parameterClass="long">
