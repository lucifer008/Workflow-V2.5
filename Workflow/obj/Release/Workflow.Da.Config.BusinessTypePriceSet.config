	   -->
	  <update id="UpdateMembercardPrice">
		  update BUSINESS_TYPE_PRICE_SET
		  set new_price=#NewPrice#
		  where BASE_BUSINESS_TYPE_PRICE_SET_ID=#BaseBusinessTypePriceSetId#
		  and Price_type=#PriceType# and Target_Id=#TargetId#
		  and company_Id=#CompanyId# and Branch_Shop_Id=#BranchShopId#
	  </update>

	  <update id="UpdateBusinessTypePrice" parameterClass="BusinessTypePriceSet" >
		  update Business_Type_Price_Set
		  set new_price=#NewPrice#,price_concession=$NewPrice$/standard_price*100
		  where id=#Id#
	  </update>
